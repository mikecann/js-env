{
  "name": "ts-env-cmd",
  "version": "1.0.0",
  "description": "Loads environment variables from a given ts file then executes the passed command",
  "bin": {
    "ts-env": "-r ts-node/register ./index.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "yarn build -w",
    "test": "node -r ts-node/register ./index.ts"
  },
  "main": "index.js",
  "repository": "git@github.com:mikecann/ts-env-cmd.git",
  "author": "Mike Cann <mike.cann@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "execa": "^1.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/node": "^11.11.3",
    "versiony-cli": "^1.3.0"
  }
}
